      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Pharmacy2UApplication"
      xmlns:animate="clr-namespace:Pharm2UAnimations;assembly=Pharm2UAnimations"

      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Admin Pharmacy Edit Page">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0"  Text="Admin Pharmacy Edit Page" FontSize="20" Background="Cornsilk"/>
        <local:AdminPharmacyEditControl Grid.Row="1"/>
    </Grid>
</animate:BasePageAnimation>
